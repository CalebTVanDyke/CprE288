package FinalProj;

import javax.swing.JPanel;

public class SpaceObject extends JPanel {

	
	private int size;
	private int coords[];
	/**
	 * Create the panel.
	 */
	public SpaceObject(int x, int y, int size) {
		this.size = size;
		coords = new int[2];
		coords[0] = x;
		coords[1] = y;
		setBounds(x, y, size, size);
		
	}

}
